<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="titleText" format="string"/>
        <attr name="backText" format="string"/>
        <attr name="backImg" format="reference"/>
        <attr name="showBackImg" format="boolean"/>
        <attr name="showBack" format="boolean"/>
        <attr name="moreText" format="string"/>
        <attr name="moreImg" format="reference"/>
        <attr name="showMoreImg" format="boolean"/>
        <attr name="showMore" format="boolean"/>
        <attr name="showViewBar" format="boolean"/>
        <attr name="barStyle" format="enum">
            <enum name="centerBar" value="1"/>
            <enum name="leftBar" value="2"/>
        </attr>
    </declare-styleable>

</resources>