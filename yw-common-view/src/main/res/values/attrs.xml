<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--导航栏Tab__navigation.NavigationTab-->
    <declare-styleable name="NavigationTab">
        <!--未选中状态图标-->
        <attr format="reference" name="tabIconNormal"/>
        <!--选中状态图标-->
        <attr format="reference" name="tabIconSelected"/>
        <!--标签文字-->
        <attr format="string" name="tabText"/>
        <!--文字大小-->
        <attr format="dimension" name="tabTextSize"/>
        <!--未选中状态颜色-->
        <attr format="color" name="textColorNormal"/>
        <!--选中状态颜色-->
        <attr format="color" name="textColorSelected"/>
        <!--角标背景颜色-->
        <attr format="color" name="badgeBackgroundColor"/>
        <!--图标文字padding-->
        <attr format="dimension" name="paddingTexwithIcon"/>
        <!--开启图标染色，默认不开启-->
        <attr name="tintEnable" format="boolean"/>
        <!--未选中状态图标染色 默认灰色-->
        <attr name="tintNormal" format="color"/>
        <!--选中状态图标染色 默认主色调-->
        <attr name="tintSelected" format="color"/>
    </declare-styleable>

