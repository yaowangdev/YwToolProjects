    <!--主要按钮文字各状态-->
    <color name="agmobile_btn_primary_text_normal">#ffffff</color>
    <color name="agmobile_btn_primary_text_pressed">#7bbdf1</color>
    <color name="agmobile_btn_primary_text_disabled">#bdd6ea</color>
