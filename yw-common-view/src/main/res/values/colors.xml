<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--↓↓↓↓↓↓Deprecated 需要逐渐替换↓↓↓↓↓↓-->
    <!--AGMobile设计统一颜色-->
    <!--无特殊说明时，应用中的白色统一使用该颜色，包括：有内容（文字、列表）的背景色、暗底色上的文字、暗底色上的内容描边等-->
    <color name="agmobile_white">#fffeff</color>        <!--新的UI中用fff-->
    <!--无内容的背景色，作为底色用于体现空白及层次感，例如：图层查询中的背景、平板抽屉背景-->
    <color name="agmobile_white_dark">#f6f6f9</color> <!--246,246,249-->
    <!--半透明白色，目前用于暗底色上的按钮点击背景-->
    <color name="agmobile_white_trans">#60fcfcfc</color>
    <!--无特殊说明时，应用中的黑色统一使用该颜色，包括：亮底色上的文字颜色-->
    <color name="agmobile_black">#565656</color>    <!--86,86,86-->
    <!--半透明黑色，目前用于亮底色上的按钮点击背景-->
    <color name="agmobile_black_trans">#50333333</color>
    <!--全透明-->
    <color name="transparent">#00000000</color>
    <!--统一蓝色-->
    <color name="agmobile_blue">#48b4ff</color>                 <!--72,180,255-->
    <!--亮灰色，用于分割线、描边、输入框提示文字-->
    <color name="agmobile_grey_0">#d3d3d3</color>               <!--193,193,193-->
    <!--暗灰色，用于搜索框描边、搜索输入框文字-->
    <color name="agmobile_grey_2">#4b4b4b</color>               <!--75,75,75-->
    <!--↑↑↑↑↑↑Deprecated 需要逐渐替换↑↑↑↑↑↑-->

    <!--主色调，如标题栏-->
    <color name="agmobile_primary">#36a3f7</color>
    <!--主色调暗，如状态栏-->
    <color name="agmobile_primary_dark">#0E7BD0</color>
    <!--突出色调，如文字按钮-->
    <color name="agmobile_accent">#67a7d9</color>
    <!--文字正文-->
    <color name="agmobile_text_body">#333333</color>
    <!--文字附属-->
    <color name="agmobile_text_caption">#999999</color>
    <!--文字tag-->
    <color name="agmobile_text_tag">#888888</color>
    <!--文字警告-->
    <color name="agmobile_text_warn">#f04443</color>
    <!--突出对比色-->
    <color name="agmobile_contrast">#f39800</color>
    <color name="agmobile_contrast_dark">#d78700</color>
    <!--文字标题栏-->
    <color name="agmobile_text_title_bar">#fff</color>
    <!--分割线-->
    <color name="agmobile_divider">#d6d6d6</color>
    <!--分割线深色-->
