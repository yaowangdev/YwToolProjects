    <!--分割线标题栏-->
    <color name="agmobile_divider_title_bar">#4aabf7</color>
    <!--表层背景(控件、内容)-->
    <color name="agmobile_surface">#ffffff</color>

    <color name="agmobile_surface_alter">#EDF6FD</color>
    <!--表层被按下时背景-->
    <color name="agmobile_surface_pressed">#d9d9d9</color>
    <!--底层背景(窗口底色)-->
