<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="rectangle" >
            <corners android:radius="@dimen/agmobile_button_radius" />
            <solid android:color="@color/agmobile_btn_warn_solid_disabled" />
            <stroke android:width="@dimen/agmobile_button_stroke" android:color="@color/agmobile_btn_warn_stroke_disabled"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" >
            <corners android:radius="@dimen/agmobile_button_radius" />
            <solid android:color="@color/agmobile_btn_warn_solid_pressed" />
            <stroke android:width="@dimen/agmobile_button_stroke" android:color="@color/agmobile_btn_warn_stroke_pressed"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" >
            <corners android:radius="@dimen/agmobile_button_radius" />
            <solid android:color="@color/agmobile_btn_warn_solid_normal" />
            <stroke android:width="@dimen/agmobile_button_stroke" android:color="@color/agmobile_btn_warn_stroke_normal" />
        </shape>
    </item>
</selector>
